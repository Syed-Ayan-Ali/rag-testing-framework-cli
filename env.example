# RAG CLI Tester Environment Configuration
# Copy this file to .env and update with your credentials

# Database Configuration (Supabase/PostgreSQL)
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here
DATABASE_URL=postgres://user:pass@host:port/db

# OpenAI Configuration (Optional)
OPENAI_API_KEY=your-openai-api-key-here
OPENAI_MODEL=gpt-4o
OPENAI_EMBEDDING_MODEL=text-embedding-3-small

# Custom Model Integration (OpenAI-compatible)
CUSTOM_API_KEY=ms-12345678-abcdefghijklmnop-1234
CUSTOM_ENDPOINT=https://api-inference.modelscope.cn/v1/chat/completions
CUSTOM_MODEL=Qwen/Qwen3-235B-A22B

# Local Embedding Models
EMBEDDING_MODEL=Xenova/all-MiniLM-L6-v2
LOCAL_EMBEDDING_MODEL=Xenova/all-MiniLM-L6-v2

# Output Configuration
OUTPUT_PATH=./rag-test-results
RAG_TEST_OUTPUT_DIR=./rag-test-results

# Test Configuration
NODE_ENV=development
TEST_TIMEOUT=30000

# Note: Keep your .env file secure and never commit it to version control
